                    }
                },
                "last_event_time": {
                    "description": "Timestamp of last event",
                    "type": "string",
                    "example": "2023-01-01T12:00:00Z"
                },
                "query_time": {
                    "description": "Query timestamp",
                    "type": "string",
                    "example": "2023-01-01T12:00:00Z"
                },
                "total_events": {
                    "description": "Total events stored",
                    "type": "integer",
                    "example": 12500
